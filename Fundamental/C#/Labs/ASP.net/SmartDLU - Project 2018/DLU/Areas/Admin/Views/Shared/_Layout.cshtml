@{ 
    if(Session["ID_TaiKhoan"] ==null)
    {
        Response.Redirect("~/admin/");
    }
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css/Back-end")
    <link rel="icon" href="~/Content/Back-end/dist/img/logo.png" type="image/x-icon">
</head>
<body class="sidebar-mini fixed skin-blue">
    <div class="wrapper">
        @RenderPage("../Block/_Header.cshtml")
        @RenderPage("../Block/_Sidebar.cshtml")
        <div class="content-wrapper">
            @RenderBody()
        </div>
        @RenderPage("../Block/_Footer.cshtml")
        @RenderPage("../Block/_Control-sidebar.cshtml")
    </div>
    @Scripts.Render("~/bundles/javascript/Back-end")
    @RenderSection("Myscripts", required: false)
    <script src="~/Scripts/Back-end/BoDe_DeThi/jquery.validate.min.js"></script>
    <script src="~/Scripts/Back-end/BoDe_DeThi/jquery.validate.unobtrusive.min.js"></script>
    <script>
        $(function () {
            $('#AlertBox').removeClass('hide');
            $('#AlertBox').delay(5000).slideUp(1000);

        });
    </script>
</body>
</html>