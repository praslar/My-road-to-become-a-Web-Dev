@using DataAccess;
@{
    ViewBag.Title = "Xem điểm thi";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var list = Model as List<GETDIEMTHIMODELVIEW>;
}
<div class="grid-content">
    <div id="div-category" class="col-1 col-1-slide col-md-2 fadeIn animated" style="overflow: hidden; padding: 0px; width: 253px;">
        <div class="jspContainer" style="width: 253px; height: 681px;">
            <div class="jspPane" style="padding: 0px; top: 0px; left: 0px; width: 253px;">
                @RenderPage("../Shared/_Menu_SinhVien.cshtml")
            </div>
        </div>
    </div>
    <div id="div-main-user" class="col-12 col-md-9 including fadeIn animated col-list-quiz">
        <!--Đề thi online ở trang chủ-->
        <div class="heading heading-white">
            <h1><i class="glyphicon glyphicon-list-alt"></i> Điểm thi của sinh viên</h1>
            <hr>
        </div>

        <div class="table-responsive">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>STT</th>
                        <th>Môn học</th>
                        <th>Học kì</th>
                        <th>Đợt thi</th>
						<th>Đề thi</th>
                        <th>Điểm</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        var x = 1;
                        foreach (var item in list)
                        {
                            <tr>
                                <th scope="row">@x</th>
                                <td>@item.TENMONHOC</td>
                                <td>@item.TENHOCKI</td>
                                <td>@item.TENDOTTHI</td>
								<td>@item.IDMADE</td>
                                <td>@item.TONGDIEM</td>
                                <td><a class="btn btn-success btn-xs">Xem</a></td>

                            </tr>
                            x = x + 1;
                        }
                    }
                </tbody>
            </table>
        </div>

    </div>


    <div id="col-12" class="col-12 col-md-12">
    </div>
    <script type="text/javascript" src="http://www.studyvn.com/assets/js/newstyle/default-main.js"></script>


</div>

